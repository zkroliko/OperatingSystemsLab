cc = gcc
cxx = g++
cflags = -std=gnu99 -pedantic -Wall -g -fPIC
lflags = -Wall 
objs = list.o
libs: liblist.so liblist.a
	
liblist.so: $(objs)
	$(cc) $(cflags) -o liblist.so list.c 
liblist.a: $(objs)
	ar rcs $@ list.o
%.o: %.c 
	$(cc) $(cflags) -c -pedantic $<	
clean:
	rm -f *.o *.a *.so $(objs) test
